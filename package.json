{
  "name": "angular-stats",
  "version": "1.2.4",
  "description": "Little utility to show stats about your angularjs app",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "main": "./dist/angular-stats.js",
  "types": "./interfaces/index.d.ts",
  "scripts": {
    "prepublish": "webpack --profile --color --env name=prod",
    "build": "webpack --progress --profile --color --env name=prod",
    "pretest": "webpack --progress --profile --color --env name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angularjs-stats"
  },
  "keywords": [
    "analysis",
    "angularjs",
    "cescon",
    "digest",
    "maurizio",
    "measure",
    "stats",
    "watcher"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@types/angular": "1.8.1",
    "@types/angular-mocks": "1.7.0",
    "@types/jasmine": "3.7.7",
    "@types/node": "15.12.2",
    "@types/webpack": "5.28.0",
    "angular": "1.8.2",
    "angular-mocks": "1.8.2",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "jasmine-core": "3.7.1",
    "jasmine-node": "3.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.1",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "4.0.1",
    "karma-safari-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.32",
    "source-map-loader": "3.0.0",
    "ts-loader": "9.2.3",
    "tslint": "6.1.3",
    "tslint-loader": "3.5.4",
    "typescript": "4.3.2",
    "webpack": "5.39.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0"
  }
}
