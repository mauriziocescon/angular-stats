{
  "name": "angular-stats",
  "version": "1.2.2",
  "description": "Little utility to show stats about your angularjs app",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": "=10.15.3",
    "npm": "=6.4.1"
  },
  "main": "./dist/angular-stats.js",
  "types": "./interfaces/index.d.ts",
  "scripts": {
    "prepublish": "webpack --profile --colors --env.name=prod",
    "build": "webpack --progress --profile --colors --env.name=prod",
    "pretest": "webpack --progress --profile --colors --env.name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angular-stats"
  },
  "keywords": [
    "analysis",
    "angularjs",
    "cescon",
    "digest",
    "maurizio",
    "measure",
    "stats",
    "watcher"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/angular": "1.6.54",
    "@types/angular-mocks": "1.7.0",
    "@types/jasmine": "3.3.10",
    "@types/node": "11.11.3",
    "@types/webpack": "4.4.26",
    "angular": "1.7.8",
    "angular-mocks": "1.7.8",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "2.0.1",
    "jasmine-core": "3.3.0",
    "jasmine-node": "1.16.2",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "2.0.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-safari-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.32",
    "source-map-loader": "0.2.4",
    "tslint": "5.14.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.3.3",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-merge": "4.2.1"
  }
}
