{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "diagnostics": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "pretty": true,
    "removeComments": false,
    "target": "ES5",
    "types": ["angular"]
  },
  "exclude": [
    "node_modules"
  ]
}